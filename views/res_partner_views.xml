<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_partner_form_edevlet" model="ir.ui.view">
        <field name="name">res.partner.form.edevlet</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="vergi_no" />
                <field name="vergi_dairesi" />
                <field name="bina_numarasi" />
            </xpath>
        </field>
    </record>
</odoo>
